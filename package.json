    "@vitejs/plugin-react-swc": "^3.2.0",
    "commander": "^10.0.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.0",
    "i18next": "^22.4.9",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.4",
    "rimraf": "^4.1.2",
    "sass": "^1.58.3",
    "storybook": "7.0.0-beta.14",
    "typescript": "^4.9.4",
    "vite": "^4.1.4",
    "vite-plugin-svgr": "^2.4.0"
  },
  "packageManager": "pnpm@7.28.0",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
