{
  "name": "@axelor/ui",
  "version": "0.0.1",
  "author": "Axelor",
  "license": "AGPL-3.0-only",
  "keywords": [
    "react",
    "components",
    "ui",
    "axelor"
  ],
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@3.3.1",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*/index.ts",
    "./*/types": "./src/*/types.ts",
    "./*/utils": "./src/*/utils.ts",
    "./core/*": "./src/core/*/index.ts",
    "./core/*/types": "./src/core/*/types.ts",
    "./core/styles": "./src/core/styles/index.tsx"
  },
  "scripts": {
    "preinstall": "node scripts/check-yarn.js",
    "format": "prettier -w '**/*.{ts,tsx,js,jsx,json,css,scss}'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "node scripts/build.js . -o dist",
    "clean": "rimraf dist"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off"
    }
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.2.0",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.3",
    "csstype": "^3.1.1",
    "focus-trap": "^7.2.0",
    "immer": "^9.0.16",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-big-calendar": "^1.5.2",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-select": "^5.7.0",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@storybook/addon-backgrounds": "7.0.0-beta.14",
    "@storybook/addon-links": "7.0.0-beta.14",
    "@storybook/addon-measure": "7.0.0-beta.14",
    "@storybook/addon-outline": "7.0.0-beta.14",
    "@storybook/addon-toolbars": "7.0.0-beta.14",
    "@storybook/addon-viewport": "7.0.0-beta.14",
    "@storybook/blocks": "7.0.0-beta.14",
    "@storybook/builder-vite": "7.0.0-beta.14",
    "@storybook/react": "7.0.0-beta.14",
    "@storybook/react-vite": "7.0.0-beta.14",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.21",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-big-calendar": "^0.38.1",
    "@types/react-dom": "^18.0.5",
    "@types/react-transition-group": "^4.4.5",
    "@vitejs/plugin-react-swc": "^3.0.1",
    "commander": "^10.0.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.0",
    "i18next": "^21.10.0",
    "prettier": "^2.8.3",
    "react-i18next": "^11.18.6",
    "rimraf": "^4.1.2",
    "sass": "^1.57.1",
    "storybook": "7.0.0-beta.14",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-svgr": "^2.4.0"
  }
}
