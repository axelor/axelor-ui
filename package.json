{
  "name": "@axelor/ui",
  "version": "0.0.13",
  "author": "Axelor",
  "license": "AGPL-3.0-only",
  "keywords": [
    "react",
    "components",
    "ui",
    "axelor"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*/index.ts",
    "./core/*": "./src/core/*/index.ts",
    "./icons/*": "./src/icons/*/index.ts"
  },
  "scripts": {
    "preinstall": "node scripts/check-pnpm.js",
    "format": "prettier -w '**/*.{ts,tsx,js,jsx,json,css,scss}'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "node scripts/build.js . -o dist",
    "clean": "rimraf dist",
    "lint": "eslint src"
  },
  "peerDependencies": {
    "react": "^18.2.0-0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0-0"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.2.1",
    "@fluentui/priority-overflow": "^9.1.0",
    "@fluentui/react-context-selector": "^9.1.23",
    "@material-symbols/font-400": "^0.6.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "color": "^4.2.3",
    "csstype": "^3.1.2",
    "dayjs": "^1.11.8",
    "focus-trap": "^7.4.3",
    "immer": "^9.0.18",
    "react-beautiful-dnd": "^13.1.1",
    "react-big-calendar": "^1.6.8",
    "react-select": "^5.7.3",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/addon-interactions": "^7.0.20",
    "@storybook/addon-links": "^7.0.20",
    "@storybook/blocks": "^7.0.20",
    "@storybook/react": "^7.0.20",
    "@storybook/react-vite": "^7.0.20",
    "@storybook/testing-library": "^0.1.0",
    "@types/color": "^3.0.3",
    "@types/react": "^18.2.9",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-big-calendar": "^0.38.4",
    "@types/react-dom": "^18.2.4",
    "@types/react-transition-group": "^4.4.6",
    "@vitejs/plugin-react": "^4.0.0",
    "commander": "^10.0.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "i18next": "^22.5.1",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "rimraf": "^5.0.1",
    "sass": "^1.63.2",
    "storybook": "^7.0.20",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-svgr": "^3.2.0"
  },
  "packageManager": "pnpm@8.6.6",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "@eslint/*"
      ]
    }
  }
}
