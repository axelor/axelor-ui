{
  "name": "@axelor/ui",
  "version": "0.0.1",
  "author": "Axelor",
  "license": "AGPL-3.0-only",
  "keywords": [
    "react",
    "components",
    "ui",
    "axelor"
  ],
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@3.3.0",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*/index.ts",
    "./*/types": "./src/*/types.ts",
    "./*/utils": "./src/*/utils.ts",
    "./core/*": "./src/core/*/index.ts",
    "./core/*/types": "./src/core/*/types.ts",
    "./core/styles": "./src/core/styles/index.tsx"
  },
  "scripts": {
    "preinstall": "node scripts/check-yarn.js",
    "format": "prettier -w '**/*.{ts,tsx,js,jsx,json,css,scss}'",
    "storybook": "storybook dev --disable-telemetry -p 6006",
    "build-storybook": "storybook --disable-telemetry build",
    "build": "node scripts/build.js . -o dist",
    "clean": "rimraf dist"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off"
    }
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.2.0",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.2",
    "focus-trap": "^7.1.0",
    "immer": "^9.0.16",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-big-calendar": "^1.5.2",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-select": "^5.6.1",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^7.0.0-alpha.52",
    "@storybook/addon-essentials": "^7.0.0-alpha.52",
    "@storybook/addon-interactions": "^7.0.0-alpha.52",
    "@storybook/addon-links": "^7.0.0-alpha.52",
    "@storybook/addons": "^7.0.0-alpha.52",
    "@storybook/builder-vite": "^7.0.0-alpha.52",
    "@storybook/channel-postmessage": "^7.0.0-alpha.52",
    "@storybook/channel-websocket": "^7.0.0-alpha.52",
    "@storybook/client-api": "^7.0.0-alpha.52",
    "@storybook/node-logger": "^7.0.0-alpha.52",
    "@storybook/preview-web": "^7.0.0-alpha.52",
    "@storybook/react": "^7.0.0-alpha.52",
    "@storybook/react-vite": "^7.0.0-alpha.25",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.21",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-big-calendar": "^0.38.1",
    "@types/react-dom": "^18.0.5",
    "@types/react-transition-group": "^4.4.5",
    "@vitejs/plugin-react": "^2.2.0",
    "commander": "^9.4.1",
    "csstype": "^3.1.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "i18next": "^21.9.2",
    "prettier": "^2.8.0",
    "react-i18next": "^11.18.6",
    "rimraf": "^3.0.2",
    "sass": "^1.56.1",
    "storybook": "^7.0.0-alpha.52",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-svgr": "^2.2.2"
  }
}
