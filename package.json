{
  "name": "@axelor/ui",
  "version": "0.5.4",
  "author": "Axelor",
  "license": "AGPL-3.0-only",
  "keywords": [
    "react",
    "components",
    "ui",
    "axelor"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*/index.ts",
    "./core/*": "./src/core/*/index.ts",
    "./icons/*": "./src/icons/*/index.ts"
  },
  "engines": {
    "node": ">=22.17"
  },
  "scripts": {
    "preinstall": "node scripts/check-pnpm.js",
    "format": "prettier -w '**/*.{ts,tsx,js,jsx,json,css,scss}'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "node scripts/build.js . -o dist",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "lint": "eslint src"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.4.0",
    "@floating-ui/react": "^0.27.14",
    "@fluentui/priority-overflow": "^9.1.16",
    "@fluentui/react-context-selector": "^9.2.7",
    "@hello-pangea/dnd": "^18.0.1",
    "@material-symbols/font-400": "^0.35.2",
    "@material-symbols/metadata": "^0.35.2",
    "@types/qrcode": "^1.5.5",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "color": "^5.0.2",
    "csstype": "^3.1.3",
    "dayjs": "^1.11.18",
    "focus-trap": "^7.6.5",
    "immer": "^10.1.3",
    "jsbarcode": "^3.12.1",
    "qrcode": "^1.5.4",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.35.0",
    "@storybook/addon-links": "^9.1.6",
    "@storybook/addon-docs": "^9.1.6",
    "@storybook/react": "^9.1.6",
    "@storybook/react-vite": "^9.1.6",
    "@types/node": "^22.18.5",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-transition-group": "^4.4.12",
    "@vitejs/plugin-react": "^4.7.0",
    "commander": "^14.0.1",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.2",
    "globals": "^16.4.0",
    "i18next": "^25.5.2",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.7.3",
    "rimraf": "^6.0.1",
    "sass": "1.77.8",
    "storybook": "^9.1.6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.5",
    "vite-plugin-svgr": "^4.3.0"
  },
  "packageManager": "pnpm@10.16.1+sha512.0e155aa2629db8672b49e8475da6226aa4bdea85fdcdfdc15350874946d4f3c91faaf64cbdc4a5d1ab8002f473d5c3fcedcd197989cf0390f9badd3c04678706",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "@eslint/*"
      ]
    },
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
