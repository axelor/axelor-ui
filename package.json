    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.3",
    "csstype": "^3.1.1",
    "focus-trap": "^7.3.1",
    "immer": "^9.0.18",
    "moment": "^2.29.4",
    "react-beautiful-dnd": "^13.1.1",
    "react-big-calendar": "^1.6.8",
    "react-select": "^5.7.0",
    "react-transition-group": "^4.4.5"
