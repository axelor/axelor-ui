{
  "name": "@axelor/ui",
  "version": "0.0.19",
  "author": "Axelor",
  "license": "AGPL-3.0-only",
  "keywords": [
    "react",
    "components",
    "ui",
    "axelor"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*/index.ts",
    "./core/*": "./src/core/*/index.ts",
    "./icons/*": "./src/icons/*/index.ts"
  },
  "scripts": {
    "preinstall": "node scripts/check-pnpm.js",
    "format": "prettier -w '**/*.{ts,tsx,js,jsx,json,css,scss}'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "node scripts/build.js . -o dist",
    "clean": "rimraf dist",
    "lint": "eslint src"
  },
  "peerDependencies": {
    "react": "^18.2.0-0",
    "react-dom": "^18.2.0-0"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.2.1",
    "@floating-ui/react": "^0.25.2",
    "@fluentui/priority-overflow": "^9.1.5",
    "@fluentui/react-context-selector": "^9.1.34",
    "@material-symbols/font-400": "^0.12.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "color": "^4.2.3",
    "csstype": "^3.1.2",
    "dayjs": "^1.11.10",
    "focus-trap": "^7.5.2",
    "immer": "^10.0.2",
    "react-beautiful-dnd": "^13.1.1",
    "react-big-calendar": "^1.8.4",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.4.3",
    "@storybook/addon-interactions": "^7.4.3",
    "@storybook/addon-links": "^7.4.3",
    "@storybook/blocks": "^7.4.3",
    "@storybook/react": "^7.4.3",
    "@storybook/react-vite": "^7.4.3",
    "@storybook/testing-library": "^0.2.1",
    "@types/color": "^3.0.4",
    "@types/node": "^20.7.1",
    "@types/react": "^18.2.21",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-big-calendar": "^1.6.4",
    "@types/react-dom": "^18.2.7",
    "@types/react-transition-group": "^4.4.6",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vitejs/plugin-react": "^4.0.4",
    "commander": "^11.0.0",
    "eslint": "^8.49.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "i18next": "^23.5.1",
    "prettier": "^3.0.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "rimraf": "^5.0.1",
    "sass": "^1.68.0",
    "storybook": "^7.4.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-svgr": "^3.2.0"
  },
  "packageManager": "pnpm@8.7.6",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "@eslint/*"
      ]
    }
  }
}
