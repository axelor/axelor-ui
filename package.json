{
  "name": "@axelor/ui",
  "version": "0.0.13",
  "author": "Axelor",
  "license": "AGPL-3.0-only",
  "keywords": [
    "react",
    "components",
    "ui",
    "axelor"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
